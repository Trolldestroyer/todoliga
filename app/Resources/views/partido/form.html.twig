{% extends '::base.html.twig' %}


{% block body %}
    <h1>Partido</h1>
    <div>
        <form method="post" action="{{ action }}" novalidate="novalidate">
            <div>
                <ul>
                    {% for error in form.vars.errors %}
                        <li>{{ error.message }}</li>
                    {% endfor %}
                </ul>
            </div>
            <div>{{ form_errors(form.nombre) }}{{ form_label(form.nombre) }}: {{ form_widget(form.nombre) }}</div>
            <div>{{ form_errors(form.primerEquipo) }}{{ form_label(form.primerEquipo) }}: {{ form_widget(form.primerEquipo) }}</div>
            <div>{{ form_errors(form.segundoEquipo) }}{{ form_label(form.segundoEquipo) }}: {{ form_widget(form.segundoEquipo) }}</div>
            <div>{{ form_errors(form.texto) }}{{ form_label(form.texto) }}: {{ form_widget(form.texto) }}</div>
            <div>{{ form_errors(form.fecha_encuentro) }}{{ form_label(form.fecha_encuentro) }}: {{ form_widget(form.fecha_encuentro) }}</div>
            {{ form_rest(form) }}
            <div><input type="submit" value="Ok"></div>
        </form>
    </div>

{% endblock %}